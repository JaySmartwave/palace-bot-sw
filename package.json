    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.4",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "^2.3.4",
    "morgan": "^1.7.0",
    "nock": "^8.0.0",
    "null-loader": "0.1.1",
    "plop": "1.5.0",
    "react-css-modules": "^3.7.6",
    "redux-devtools": "^3.0.1",
