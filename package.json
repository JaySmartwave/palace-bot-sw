{
  "name": "palace-bot",
  "version": "1.0.1",
  "description": "A highly scalable and interactive React corporate dashboard, built in a component oriented manner",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "engines": {
    "node": "5.2.0",
    "npm": "3.10.7"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start config/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "build": "./node_modules/.bin/webpack --display-error-details",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "generate": "plop --plopfile config/generators/index.js",
    "generator": "npm run generate",
    "generate:component": "plop --plopfile config/generators/index.js component",
    "generate:container": "plop --plopfile config/generators/index.js container",
    "generate:page": "plop --plopfile config/generators/index.js page",
    "lint": "eslint . --ext .js --ext .jsx; exit 0",
    "postinstall": "webpack --config ./webpack.config.babel.js --progress",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "start": "node server.js",
    "clean": "rm -rf app/dist app/build",
    "setup": "npm install",
    "serve:bundle": "cross-env NODE_ENV=production PORT=1337 node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCCollins/corporate-dashboard.git"
  },
  "keywords": [
    "boilerplate",
    "redux",
    "react",
    "webpack",
    "sass",
    "css modules"
  ],
  "author": "Ryan Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCCollins/corporate-dashboard/issues"
  },
  "homepage": "https://github.com/RyanCCollins/corporate-dashboard#readme",
  "dependencies": {
    "actions": "^1.3.0",
    "apollo-client": "^0.4.15",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "latest",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-relay-plugin": "^0.9.3",
    "components": "^0.1.0",
    "cors": "^2.8.1",
    "css-loader": "^0.23.0",
    "csvtojson": "^1.0.0",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.9.0",
    "grommet": "1.2.1",
    "grommet-udacity": "^0.1.2",
    "history": "^1.14.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "json-loader": "^0.5.4",
    "json2csv": "^3.7.3",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "node-sass": "^3.4.2",
    "npm-install-webpack-plugin": "^4.0.3",
    "partybot-http-client": "0.0.1-beta-66",
    "rc-time-picker": "^2.2.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-update": "^15.3.2",
    "react-apollo": "^0.5.5",
    "react-datepicker": "^0.40.0",
