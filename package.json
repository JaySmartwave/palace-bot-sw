    "react-foundation": "^0.6.8",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-relay": "^0.9.3",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-select": "^1.0.0-rc.3",
    "react-tagsinput": "^3.14.0",
    "redux": "^3.5.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.2.0",
    "redux-thunk": "^1.0.0",
    "resolve-url-loader": "^1.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
